{
    "source": "./source",
    "destination": "./docs",
    "index": "../README.md",
    "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "test": {
          "source": "./tests/",
          "interfaces": ["describe", "it", "context", "suite", "test"],
          "includes": ["(spec|Spec|test|Test)\\.js$"],
          "excludes": ["\\.config\\.js$"]
        },
        "brand": {
          "title": "Blockchain",
          "description": "This is the documentation of it",
          "repository": "https://github.com/BirgitPohl/zkSystemsCodeChallenge"
        },
        "manual": {
          "index": "../README.md",
          "asset": "",
          "files": [
            "./manual.md"
          ]
        }
      }
    }]
  }